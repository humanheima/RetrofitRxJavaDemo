<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data></data>

    <androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_rx_java"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.activity.RxJavaOperatorActivity">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/btn_parallel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/parallel"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_transform"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/transform"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_ref_count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/ref_count"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_connect"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/connect"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_join"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/join"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_take_while"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/takeWhile"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_take_until"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/takeUntil"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_skip_while"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/skipWhile"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_skip_until"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/skipUntil"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_sequence_equal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/sequenceEqual"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_amb"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/amb"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_default_if_empty"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/btn_default_if_empty"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_create"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/create"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_take"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/take"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/btn_defer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="click"
                android:text="@string/defer"
                android:textAllCaps="false" />

            <Button
